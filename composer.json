    "keywords": [
    "WhatsApp",
    "API",
    "PHP",
    "Laravel",
    "messaging",
    "WhatsAppSender",
    "chatAPI",
    "composer",
    "integration",
    "library",
    "framework",
    "SMS",
    "notification",
    "automation",
    "webhook",
    "REST",
    "communication",
    "texting",
    "sdk",
    "gateway"
    ],
    "autoload": {
        "psr-4": {
            "Eagleminds\\WhatsappSender\\": "src/"
        }
    },
    "authors": [
        {
            "name": "The Eagleminds",
            "email": "eaglemindsdev@gmail.com"
        }
    ],
    "extra": {
